                                            <ul x-ref="gallery" id="gallery"
                                                class="grid grid-cols-2 gap-5 lg:grid-cols-5">
                                                <li>
                                                    <% if (SextaFeira && SextaFeira.fotoNivelCombustivel) { %>
                                                        <img x-on:click="imageGalleryOpen"
                                                            src="<%= SextaFeira && SextaFeira.fotoNivelCombustivel ? SextaFeira.fotoNivelCombustivel : 'Não possui imagem' %>"
